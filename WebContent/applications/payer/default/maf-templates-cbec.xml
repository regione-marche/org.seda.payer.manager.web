<?xml version="1.0" encoding="UTF-8"?>
<templates xmlns="http://template.core.maf.j2ee5.seda.com"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://template.core.maf.j2ee5.seda.com ../../../WEB-INF/xsd/maf-templates.xsd">
	<default>/applications/templates/default/default.jsp</default>
	<bundle>org.seda.payer.manager.resources.TemplateStrings</bundle>
	<screen name="default">
		 <parameter key="pagetitle" value="title.menu" direct="true"  />
  		 <parameter key="header" value="/applications/templates/shared/header.jsp"/>
    	 <parameter key="menu" value="/applications/templates/shared/menu.jsp" />
    	 <parameter key="main" value="/applications/payer/default/include/mainempty.jsp" />       
  	     <parameter key="footer" value="/applications/templates/shared/footer.jsp"/>
	</screen>

	<screen name="sceltaProfilo">
		 <parameter key="pagetitle" value="title.sceltaProfilo" direct="true"  />
  		 <parameter key="header" value="/applications/templates/shared/header.jsp"/>
    	 <parameter key="main" value="/applications/payer/default/include/sceltaProfilo.jsp" />       
  	     <parameter key="footer" value="/applications/templates/shared/footer.jsp"/>
	</screen>

  <screen name="message" template="default">
	<parameter key="pagetitle" value="manager.default.errore.title" direct="true"/>
    <parameter key="header" value="/applications/templates/shared/header.jsp" />
    <parameter key="main" value="/applications/payer/default/include/message.jsp" />
    <parameter key="footer" value="/applications/templates/shared/footer.jsp" />
  </screen>
	
</templates>
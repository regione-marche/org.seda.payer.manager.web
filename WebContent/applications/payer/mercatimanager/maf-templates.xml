<?xml version="1.0" encoding="UTF-8"?>
<templates xmlns="http://template.core.maf.j2ee5.seda.com"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://template.core.maf.j2ee5.seda.com ../../../WEB-INF/xsd/maf-templates.xsd">

  <default>/applications/templates/default/default.jsp</default>

  <bundle>org.seda.payer.manager.resources.TemplateStrings</bundle>
  
  <template name="errori">
	<url>/applications/templates/default/error_template.jsp</url>
  </template>

  <screen name="mercatiMonitoraggio" template="default">
    <parameter key="pagetitle" value="manager.mercatimonitoraggio.search.title" direct="true" />
  	<parameter key="header" value="/applications/templates/shared/header.jsp"/>
    <parameter key="menu" value="/applications/templates/shared/menu.jsp" />
    <parameter key="main" value="/applications/payer/mercatimanager/include/MercatiMonitoraggio.jsp" />
    <parameter key="footer" value="/applications/templates/shared/footer.jsp"/>
  </screen>
  
  <screen name="mercatimanager" template="default">
    <parameter key="pagetitle" value="manager.mercatimonitoraggio.search.title" direct="true" />
  	<parameter key="header" value="/applications/templates/shared/header.jsp"/>
    <parameter key="menu" value="/applications/templates/shared/menu.jsp" />
    <parameter key="main" value="/applications/payer/mercatimanager/include/MercatiManager.jsp" />
    <parameter key="footer" value="/applications/templates/shared/footer.jsp"/>
  </screen>

</templates>
